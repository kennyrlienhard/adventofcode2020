    "copyfiles": "^2.4.1",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.6.3",
    "husky": "^9.0.10",
    "jest": "^29.7.0",
    "jest-watch-select-projects": "^2.0.0",
